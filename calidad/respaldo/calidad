CREATE TABLE `ap` (
  `idac` int(5) NOT NULL AUTO_INCREMENT,
  `conforme` varchar(900) NOT NULL,
  `folio` varchar(10) NOT NULL,
  `dete` varchar(20) NOT NULL,
  `dete1` varchar(20) NOT NULL,
  `dete2` varchar(20) NOT NULL,
  `dete3` varchar(20) NOT NULL,
  `dete4` varchar(20) NOT NULL,
  `otra` varchar(25) DEFAULT NULL,
  `fechanoti` date NOT NULL,
  `area` varchar(60) NOT NULL,
  `auditor` varchar(50) NOT NULL,
  `nombreno` varchar(50) NOT NULL,
  `nombreres` varchar(50) NOT NULL,
  `nombre` varchar(50) NOT NULL,
  `accionin` varchar(300) NOT NULL,
  `resultado` varchar(300) NOT NULL,
  `accion1` varchar(150) DEFAULT NULL,
  `res1` varchar(30) NOT NULL,
  `fecha1` date NOT NULL,
  `accion2` varchar(150) DEFAULT NULL,
  `res2` varchar(30) NOT NULL,
  `fecha2` date NOT NULL,
  `accion3` varchar(150) DEFAULT NULL,
  `res3` varchar(30) NOT NULL,
  `fecha3` date NOT NULL,
  `accion4` varchar(150) DEFAULT NULL,
  `res4` varchar(30) NOT NULL,
  `fecha4` date NOT NULL,
  `accion5` varchar(150) DEFAULT NULL,
  `res5` varchar(30) NOT NULL,
  `fecha5` date NOT NULL,
  `porque1` varchar(200) NOT NULL,
  `porque2` varchar(200) NOT NULL,
  `porque3` varchar(200) NOT NULL,
  `porque4` varchar(200) NOT NULL,
  `porque5` varchar(200) NOT NULL,
  `otratec` varchar(200) NOT NULL,
  `causa1` varchar(20) NOT NULL,
  `causa2` varchar(20) NOT NULL,
  `causa3` varchar(20) NOT NULL,
  `causa4` varchar(20) NOT NULL,
  `causa5` varchar(20) NOT NULL,
  `causa6` varchar(20) NOT NULL,
  `subcausa1` varchar(100) NOT NULL,
  `subcausa2` varchar(100) NOT NULL,
  `subcausa3` varchar(100) NOT NULL,
  `subcausa4` varchar(100) NOT NULL,
  `subcausa5` varchar(100) NOT NULL,
  `subcausa6` varchar(100) NOT NULL,
  `efecto` varchar(80) NOT NULL,
  `tipo` varchar(3) NOT NULL,
  `efectivo` varchar(200) NOT NULL,
  `auditorarea` varchar(50) NOT NULL,
  `fechaarea` date NOT NULL,
  `veriefectivo` varchar(200) NOT NULL,
  `auditorveri` varchar(50) NOT NULL,
  `fechaveri` date NOT NULL,
  `var1` varchar(10) NOT NULL,
  `var2` varchar(10) NOT NULL,
  `area1` varchar(60) DEFAULT NULL,
  `area2` varchar(60) DEFAULT NULL,
  `area3` varchar(60) DEFAULT NULL,
  `nombreres1` varchar(50) DEFAULT NULL,
  `nombreres2` varchar(50) DEFAULT NULL,
  `nombreres3` varchar(50) DEFAULT NULL,
  `cancel` varchar(3) DEFAULT NULL,
  PRIMARY KEY (`idac`)
) ENGINE=InnoDB AUTO_INCREMENT=45 DEFAULT CHARSET=latin1;
